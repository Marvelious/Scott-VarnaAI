# FwChange Production Environment Variables
# VPS: [YOUR_VPS_IP]
# Path: Docker container fwchange-backend:/app/.env

# Database
DATABASE_URL=postgresql://postgres:CHANGE_ME@postgres:5432/fwchange

# Redis
REDIS_URL=redis://redis:6379

# Security (Generate with: openssl rand -hex 32)
JWT_SECRET=CHANGE_ME_GENERATE_WITH_OPENSSL
JWT_EXPIRATION=24h
SESSION_SECRET=CHANGE_ME_GENERATE_WITH_OPENSSL
SECRET_KEY=CHANGE_ME_GENERATE_WITH_OPENSSL

# Settings
DISABLE_SIGNUP=true
NODE_ENV=production
DEBUG=false
