# RetirementAI Production Environment Variables
# VPS: [YOUR_VPS_IP]
# Path: /opt/demos/retirementai/.env

# Node Environment
NODE_ENV=production
PORT=3000

# Database (PostgreSQL with pgvector)
DATABASE_URL=postgresql://retirement_ai:CHANGE_ME_DB_PASSWORD@localhost:5432/retirement_ai?sslmode=disable
POSTGRES_PASSWORD=CHANGE_ME_DB_PASSWORD

# Redis (AI Caching & BullMQ) - with password
REDIS_URL=redis://:CHANGE_ME_REDIS_PASSWORD@localhost:6379

# Security Keys (Generate with: openssl rand -hex 32)
SERVER_ENCRYPTION_KEY=CHANGE_ME_GENERATE_WITH_OPENSSL
JWT_SECRET=CHANGE_ME_GENERATE_WITH_OPENSSL
NEXTAUTH_SECRET=CHANGE_ME_GENERATE_WITH_OPENSSL
NEXTAUTH_URL=https://your-domain.com

# AI Providers
OPENAI_API_KEY=sk-your-openai-key
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key
